<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="DatabaseBundle\Entity\Diplome" table="diplome">
    <indexes>
      <index name="diplome_ibfk_1" columns="etablissement"/>
      <index name="filiere" columns="filiere"/>
      <index name="domaine" columns="domaine"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="code" type="bigint" column="code" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="score" type="integer" column="score" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="capacite" type="integer" column="capacite" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="etablissement" target-entity="Etablissements" fetch="LAZY">
      <join-columns>
        <join-column name="etablissement" referenced-column-name="designation"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="filiere" target-entity="Filieres" fetch="LAZY">
      <join-columns>
        <join-column name="filiere" referenced-column-name="designation"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="domaine" target-entity="Domaine" fetch="LAZY">
      <join-columns>
        <join-column name="domaine" referenced-column-name="designation"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
